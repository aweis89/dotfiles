{
  "$schema": "https://raw.githubusercontent.com/sumneko/vscode-lua/master/setting/schema.json",
  "Lua.runtime.version": "Lua 5.4",
  "Lua.runtime.path": [
    "?.lua",
    "?/init.lua",
    "dot_hammerspoon/?.lua",
    "dot_hammerspoon/?/init.lua",
    "dot_hammerspoon/lua/?.lua",
    "dot_hammerspoon/lua/?/init.lua"
  ],
  "Lua.workspace.library": [
    "./dot_hammerspoon",
    "./dot_hammerspoon/Spoons/EmmyLua.spoon/annotations"
  ],
  "Lua.workspace.checkThirdParty": false,
  "Lua.diagnostics.globals": ["hs", "spoon"]
}

